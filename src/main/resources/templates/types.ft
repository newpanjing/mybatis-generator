${requestUrl}
<#list columns as item>
    <#assign str="" >
    <#assign typeName=item.typeName?uncap_first >
    <#if item.typeName =="Date">
        <#assign typeName='long' >
        <#assign str=",时间戳" >
    </#if>
    <#assign require="Y">
    <#if item.fieldName=="id">
    <#assign require="N">
    <#elseif item.fieldName=="userId">
    <#assign require="N">
    </#if>
${item.fieldName}   ${typeName}    ${require}    ${item.remark}${str}
</#list>